{
  "name": "kanban-app",
  "version": "1.0.0",
  "scripts": {
    "start": "cd backend && node server.js",
    "dev": "concurrently \"cd backend && npm run dev\" \"cd frontend && npm run dev\"",
    "build": "cd frontend && npm run build",
    "install-all": "npm install && cd backend && npm install && cd ../frontend && npm install",
    "docker-build": "docker build -t kanban-collab-project .",
    "docker-run": "docker run -p 4000:3001 kanban-collab-project"
  },
  "devDependencies": {
    "concurrently": "^8.2.0"
  },
  "dependencies": {
    "axios": "^1.12.2",
    "dotenv": "^16.3.1",
    "pg": "^8.16.3",
    "redis": "^5.8.2"
  }
}
